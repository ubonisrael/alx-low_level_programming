#include "main.h"
#include <stdio.h>

/**
 * main - prints the first 59 fibonacci numbers starting from 1 and 2
 *
 * Return: 0
 */

int main(void)
{
	long long int x;
	long long int y;
	long long int z;
	int count;

	x = 1;
	y = 2;
	count = 2;
	printf("%lld, %lld, ", x, y);
	while (count < 51)
	{
		z = x + y;
		printf("%lld", z);
		if (count < 50)
			printf(", ");
		x = y;
		y = z;
		count++;
	}
	printf("\n");
	return (0);
}
