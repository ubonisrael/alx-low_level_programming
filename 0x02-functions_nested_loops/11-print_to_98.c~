#include "main.h"

/**
 * print_to_98 - prints all natural numbers form the argument to 98
 * @n: argument
 *
 * Return: void
 */

void print_to_98(int n)
{
	if (n < 98)
	{
		while (n < 98)
		{
			if (n < 10)
			{
				if (n < 0)
				{
					_putchar('-');
					_putchar('0' + (-1 * n));
				}
				else
					_putchar('0' + n);
			}
			else
			{
				_putchar('0' + (n / 10));
				_putchar('0' + (n % 10));
			}
			_putchar(',');
			_putchar(' ');
			n++;
		}
	}
	else if (n > 98)
	{
		while (n > 98)
		{
			if (n > 99)
			{
				_putchar('0' + (n / 100));
				_putchar('0' + ((n % 100) / 10));
				_putchar('0' + ((n % 100) % 10));
			}
			else
			{
				_putchar('0' + (n / 10));
				_putchar('0' + (n % 10));
			}
			_putchar(',');
			_putchar(' ');
			n--;
		}
	}
	_putchar('0' + (n / 10));
	_putchar('0' + (n % 10));
	_putchar('\n');
}
